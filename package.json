{
  "name": "snippets",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run prod && node dist/server/snippets.js",
    "dev": "npm-run-all --parallel watch:*",
    "prod": "rm -rf dist && NODE_ENV=production webpack -p && cp -rf public dist && cp package.json dist/server",
    "watch:client": "webpack-dev-server",
    "watch:server": "nodemon ./server/app.js --exec babel-node --presets es2015,stage-2"
  },
  "author": "Sarpreet Singh Buttar",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "file-loader": "^1.1.5",
    "monaco-editor": "^0.10.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mongoose": "^4.13.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-monaco-editor": "^0.12.0"
  }
}